while not game:IsLoaded() do
    wait()
end
local libary = loadstring(game:HttpGet("https://pastebin.com/raw/RvJ0qewm", true))()
local main = libary:CreateWindow("AUTO")

local mod = main:Button("Auto Click", function()
_G.attack_enabled = not _G.attack_enabled
while _G.attack_enabled do
local args = {
    [1] = "attack"
}
game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
wait(0.1)
end
end)

local mod = main:Button("Auto Skill", function()
_G.skill_enabled = not _G.skill_enabled
while _G.skill_enabled do
local args = {
    [1] = "fire_ability",
    [2] = 1
}

game:GetService("ReplicatedStorage").Shared.events.RemoteFunction:InvokeServer(unpack(args))
wait(1)
local args = {
    [1] = "fire_ability",
    [2] = 2
}

game:GetService("ReplicatedStorage").Shared.events.RemoteFunction:InvokeServer(unpack(args))
wait(1)
local args = {
    [1] = "fire_ability",
    [2] = 3
}

game:GetService("ReplicatedStorage").Shared.events.RemoteFunction:InvokeServer(unpack(args))
wait(1)
end
end)

local mod = main:Button("Anti Afk", function()
loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-Anti-Afk-Remastered-Script-or-Diffrent-Cool-UI-2097"))()
end)

local main = libary:CreateWindow("EGG1")

local mod = main:Button("Namequi", function()
_G.village_enabled = not _G.village_enabled
while _G.village_enabled do
local args = {
    [1] = "spin",
    [2] = "champions",
    [3] = "one",
    [4] = "namequi"
}

game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
wait(0.1)
end
end)

